{
  "global": {
    "font-family": {
      "body": {
        "value": "Equitan Sans",
        "type": "fontFamilies"
      }
    },
    "font-weight": {
      "regular": {
        "value": "400",
        "type": "fontWeights"
      }
    },
    "line-height": {
      "loose": {
        "value": "150%",
        "type": "lineHeights"
      }
    },
    "font-size": {
      "base": {
        "value": "16",
        "type": "fontSizes"
      }
    },
    "typography": {
      "paragraph": {
        "value": {
          "fontFamily": "{font-family.body}",
          "fontWeight": "{font-weight.regular}",
          "lineHeight": "{line-height.loose}",
          "fontSize": "{font-size.base}"
        },
        "type": "typography"
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global"
    ]
  }
}